{
    "manifest_version": 3,
    "name": "Quotation Marks",
    "version": "1.0",
    "description": "A browser extension to copy quotation marks.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png"
      }
    },
    "permissions": ["clipboardWrite"]
  }